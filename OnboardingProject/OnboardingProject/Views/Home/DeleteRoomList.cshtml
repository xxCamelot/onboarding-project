@model System.Collections.Generic.List<RoomModel>

@{
    ViewData["Title"] = "Delete existing Room";
}

<h1>@ViewData["Title"]</h1>

<p>Select a room to delete it.</p>

@foreach (var item in Model)
{
    <div>
        @Html.ActionLink(item.description, "DeleteConfirmation", "Home", new { ID = item.roomID, index = 2 })
        <hr />
    </div>
}