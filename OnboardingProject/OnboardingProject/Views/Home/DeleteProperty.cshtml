@model System.Collections.Generic.List<PropertyDataModel>

@{
    ViewData["Title"] = "Delete existing Property";
}

<h1>@ViewData["Title"]</h1>

<p>Select a property to delete it from the list.</p>

@foreach (var item in Model)
{
    <div>
        @Html.ActionLink(item.Address, "DeleteConfirmation", "Home", new { ID = item.Id , index = 1})
        <hr />
    </div>
}